using UnityEngine;

public class BackgroundSpawner : Spawner
{
    //==========================================Variable==========================================
    private static BackgroundSpawner instance;
    public static BackgroundSpawner Instance => instance;

    //===========================================Unity============================================
    protected override void Awake()
    {
        if (instance != null)
        {
            Debug.LogError("There is more than one BackgroundSpawner in the scene", transform.gameObject);
            return;
        }

        instance = this;
    }
}
